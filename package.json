{
  "name": "genum-openapi",
  "version": "0.1.0",
  "description": "Generate Typescript enums from openapi",
  "main": "./src/cli.ts",
  "repository": "git@github.com:mrdannael/genum-openapi.git",
  "author": "mrdannael",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/ui": "^0.32.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.2.6",
    "execa": "^7.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "scripts": {
    "start": "ts-node --esm ./src/cli.ts",
    "dev": "tsc --watch",
    "build": "rimraf ./bin && rollup -c rollup.config.js && chmod +x ./bin/cli.js",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "prettier": "prettier --write .",
    "typecheck": "tsc --noemit",
    "lint:ts": "eslint '**/*.{js,jsx,ts,tsx}' --max-warnings 0 --cache",
    "prepare": "husky install"
  },
  "bin": {
    "genum-openapi": "./bin/cli.js"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "openapi-types": "^12.1.3",
    "yaml": "^2.3.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:ts --fix",
      "yarn prettier",
      "yarn typecheck",
      "yarn vitest related --run"
    ]
  }
}
