{
  "name": "genum-openapi",
  "version": "0.1.0",
  "description": "Generate Typescript enums from openapi",
  "main": "index.js",
  "repository": "git@github.com:mrdannael/genum-openapi.git",
  "author": "mrdannael",
  "license": "MIT",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "scripts": {
    "start": "ts-node --esm ./src/cli.ts",
    "dev": "tsc --watch",
    "build": "rimraf ./dist && tsc",
    "test": "exit 0",
    "lint:ts": "eslint 'src/**/*.{js,jsx,ts,tsx}' --max-warnings 0 --cache",
    "prepare": "husky install"
  },
  "bin": {
    "genum-openapi": "./dist/cli.js"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "openapi-types": "^12.1.3",
    "yaml": "^2.3.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:ts --fix",
      "yarn test"
    ]
  }
}
